<!-- Product -->
<section class="bg0 p-t-23 p-b-140">
    <div class="container">
        <div class="p-b-10">
            <h3 class="ltext-103 cl5">
                Product Overview
            </h3>
        </div>
        <!-- <% cate.forEach(function(ca){ %>
                <h1><%= ca.name %></h1>
            <% }) %> -->
        <%   cate.forEach(function(ca){ var id = 0; %>
        <div class="category clearfix">
            <h3 class="float-left">
                <span class="maintext">
                    <%= ca.name%>
                </span>
            </h3>
            <a href="/product/<%= ca.name %>" class="float-right subtext">
                <span> See More >></span>
            </a>
        </div>
        <div class="row">
            <% product.forEach(function(pro){ if(id < 4){ if(pro.cateName == ca.name){ id ++ ; %>
            <div class="col-sm-6 col-md-4 col-lg-3 p-b-35">
                <!-- Block2 -->
                <div class="block2">
                    <div class="block2-pic hov-img0">
                        <img src="/assets/images/<%= pro.img.img1 %>" alt="IMG-PRODUCT">

                        <a href="/add-to-cart/<%= pro._id %>" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04 addCArt">
                            Add Cart
                        </a>
                    </div>

                    <div class="block2-txt flex-w flex-t p-t-14">
                        <div class="block2-txt-child1 flex-col-l ">
                            <a href="/product/<%= pro.name %>.<%= pro._id%>.<%= pro.cateName %>" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                <%= pro.name %>
                            </a>

                            <span class="stext-105 cl3">
                                $<%= pro.price %>
                            </span>
                        </div>

                        <div class="block2-txt-child2 flex-r p-t-3">
                            <a href="#" class="btn-addwish-b2 dis-block pos-relative js-addwish-b2">
                                <img class="icon-heart1 dis-block trans-04" src="/assets/images/icons/icon-heart-01.png"
                                    alt="ICON">
                                <img class="icon-heart2 dis-block trans-04 ab-t-l" src="/assets/images/icons/icon-heart-02.png"
                                    alt="ICON">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <% }}}); %>
        </div>
        <% })%>
</section>



